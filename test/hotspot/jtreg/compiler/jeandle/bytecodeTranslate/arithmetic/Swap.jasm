package compiler/jeandle/bytecodeTranslate/arithmetic;

super public class Swap {
    public static Method testIswap:"(II)I" stack 3 locals 3 {
        iload_0;
        iload_1;
        swap;
        bipush 16;
        ishl;
        iadd;
        ireturn;
    }
    public static Method testFswap:"(FF)F" stack 3 locals 3 {
        fload_0;
        fload_1;
        swap;
        ldc 2.0f;
        fmul;
        fadd;
        freturn;
    }
    public static Method testAswap:"(Ljava/lang/String;Ljava/lang/String;)Ljava/lang/String;" stack 3 locals 3 {
        aload_1;
        aload_0;
        swap;
        areturn;
    }
    public static Method testSswap:"(II)I" stack 3 locals 3 {
        iload_0;
        iload_1;
        swap;
        bipush 8;
        ishl;
        iadd;
        ireturn;
    }
    public static Method testCswap:"(II)I" stack 3 locals 3 {
        iload_0;
        iload_1;
        swap;
        bipush 8;
        ishl;
        iadd;
        ireturn;
    }
    public static Method testBswap:"(II)I" stack 3 locals 3 {
        iload_0;
        iload_1;
        swap;
        bipush 4;
        ishl;
        iadd;
        ireturn;
    }
